{
  "name": "timePlanner",
  "version": "1.0.0",
  "description": "timePlanner",
  "main": "index.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "client": "node ./node_modules/webpack/bin/webpack.js --debug --progress --colors --watch",
    "client-build": "node ./node_modules/webpack/bin/webpack.js",
    "server": "node ./index.js",
    "build": "node ./node_modules/webpack/bin/webpack.js",
    "create": "node ./node.scripts/create.module.js",
    "extract": "node ./node.scripts/jade.to.html.js && node ./node_modules/grunt/bin/grunt nggettext_extract",
    "compile": "node ./node_modules/grunt/bin/grunt nggettext_compile",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-cookies": "^1.6.0",
    "angular-gettext": "^2.3.8",
    "angular-ui-grid": "^3.2.9",
    "angular-ui-router": "^0.3.1",
    "bluebird": "^3.5.0",
    "bootstrap": "^3.3.7",
    "css-loader": "0.23.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "grunt": "^1.0.1",
    "html-loader": "^0.4.3",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "moment": "^2.18.1",
    "nib": "^1.1.2",
    "pg-promise": "^5.9.7",
    "source-map-loader": "^0.1.5",
    "style-loader": "0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "underscore": "^1.8.3",
    "url-loader": "0.5.7"
  },
  "devDependencies": {
    "angular-gettext-extract-loader": "^0.5.0",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.14.0",
    "grunt-angular-gettext": "^2.3.2",
    "npm-install-webpack-plugin": "^4.0.4",
    "webpack": "^1.13.2",
    "concurrently": "^2.2.0",
    "lite-server": "^2.2.2",
    "typescript": "^2.0.2",
    "typings": "^1.3.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2017"
    ]
  }
}
